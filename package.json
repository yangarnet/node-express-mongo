{
  "name": "rest-with-express",
  "version": "1.0.0",
  "description": "RESTful service with express and mango db",
  "main": "app.js",
  "scripts": {
    "start": "babel-node --presets es2015 app.js",
    "start-watch": " nodemon --exec 'npm start'",
    "compile": "babel app.js src -d dist --source-maps --watch",
    "build": "babel app.js src -d dist --source-maps --watch",
    "test":
      "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha **/**/*.spec.js **/**/**/*.spec.js --require babel-core/register --require babel-polyfill",
    "test-watch": "nodemon --exec 'npm test'"
  },
  "keywords": ["express", "mango"],
  "author": "gyang",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.18.2",
    "config": "^1.30.0",
    "crypto-js": "^3.1.9-1",
    "express": "4.16.2",
    "helmet": "^3.12.0",
    "jsonwebtoken": "^8.2.1",
    "moment": "2.21.0",
    "mongodb": "^3.0.6",
    "mongoose": "5.0.4",
    "morgan": "^1.9.0",
    "pug": "^2.0.3",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "^4.1.2",
    "debug": "^3.1.0",
    "expect": "^22.4.3",
    "lodash": "^4.17.5",
    "mocha": "^5.0.5",
    "node-dev": "3.1.3",
    "nodemon": "1.14.12",
    "sinon": "^4.4.9",
    "supertest": "^3.0.0"
  }
}
